<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<div class="entry-list clearfix">
	<nav>
		<ul>
			{foreach from=$menu_list item=list name=name}
			<li>
				<a href="{$list.menu_url}">
					<div style="background:url({$list.menu_img}) no-repeat center center;background-size: 50%" class="nav_img"></div>
					<span>{$list.menu_name}</span>
				</a>
			</li>
			{/foreach}
		</ul>
	</nav>
</div>